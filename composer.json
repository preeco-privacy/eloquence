{
    "name": "sofa/eloquence",
    "description": "Flexible Searchable, Mappable, Metable, Validation and more extensions for Laravel Eloquent ORM.",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/jarektkaczyk/eloquence/issues",
        "source": "https://github.com/jarektkaczyk/eloquence"
    },
    "keywords": [
        "laravel",
        "eloquent",
        "metable",
        "searchable",
        "mappable",
        "mutable"
    ],
    "authors": [
        {
            "name": "Jarek Tkaczyk",
            "email": "jarek@softonsofa.com",
            "homepage": "http://softonsofa.com/",
            "role": "Developer"
        },
        {
          "name": "Andreas Hartmann",
          "email": "info@preeco.de",
          "homepage": "https://www.preeco.de/",
          "role": "Developer"
      }
    ],
    "require": {
        "php": ">=7.4.0",
        "sofa/eloquence-base": "8.0.x-dev",
        "sofa/eloquence-metable": "8.0.x-dev",
        "sofa/eloquence-mutable": "8.0.x-dev",
        "sofa/eloquence-mappable": "8.0.x-dev",
        "sofa/eloquence-validable": "8.0.x-dev"
    },
    "require-dev": {
        "phpunit/phpunit": "4.5.0",
        "squizlabs/php_codesniffer": "2.3.3",
        "mockery/mockery": "0.9.4"
    },
    "autoload-dev": {
        "psr-4": {
            "Sofa\\Eloquence\\Tests\\": "tests"
        }
    },
    "minimum-stability": "stable",
    "scripts": {
        "test": "phpunit"
    }
}
